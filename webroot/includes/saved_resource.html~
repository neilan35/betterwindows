<script type="text/javascript">
	
	jQuery(function() {
		
		jQuery('#flickr-badge_5544fe46770ef').jflickrfeed({
			limit: 6,
			qstrings: {id: '54958895@N06'},
			itemTemplate: '<li><a class="badge" target="_blank" href="{{image_b}}">\n\
							  <img src="{{image_s}}" alt="{{title}}" /></a></li>'
		}, function() {
			
				var $bwWrapper = jQuery('.badge'),
					invertHoverEffect = false,
					hoverEffect = true,
					invertHoverEffectTouch
				
				if (black_and_white_mode == 1) {
					invertHoverEffect = true;
				}

				if (black_and_white_mode == 2) {
					hoverEffect = false;
					invertHoverEffect = true;
				}
				
				// Four Touch Devices
				if (black_and_white_mode_touch == 1) {
					invertHoverEffectTouch = false;
				}	

				if (black_and_white_mode_touch == 2) {
					invertHoverEffectTouch = true;
				}
					
				if (Modernizr.touch) {

					jQuery(window).load(function() {

						$bwWrapper.BlackAndWhite({
							hoverEffect: false, // default true
							// set the path to BnWWorker.js for a superfast implementation
							webworkerPath: '',
							// for the images with a fluid width and height
							responsive: true,
							invertHoverEffect: invertHoverEffectTouch,
							speed: {//this property could also be just speed: value for both fadeIn and fadeOut
								fadeIn: 400, // 400ms for fadeIn animations
								fadeOut: 800 // 800ms for fadeOut animations
							}
						});
					});						
					
				} else {
					
					jQuery(window).load(function () {

						$bwWrapper.BlackAndWhite({
								hoverEffect: hoverEffect, // default true
								// set the path to BnWWorker.js for a superfast implementation
								webworkerPath: '',
								// for the images with a fluid width and height 
								responsive: true,
								invertHoverEffect: invertHoverEffect,
								speed: {//this property could also be just speed: value for both fadeIn and fadeOut
									fadeIn: 400, // 400ms for fadeIn animations
									fadeOut: 800 // 800ms for fadeOut animations
								}
							});
					});	
					
				} 

		});
		
	});
	
</script>

